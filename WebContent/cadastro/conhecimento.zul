				<!--				<tab id="tab3" label="Destinatário" closable="false" />-->
				<tab id="tab4" label="Consig/Redesp/Parceiros"
					closable="false" />
				<tab label="Mercadoria" closable="false" />
				<tab label="Cáculo Frete" closable="false" />
				<tab label="Motorista" closable="false" />
				<tab label="Financeiro" closable="false" />
				<tab label="CT-e" closable="false" />
