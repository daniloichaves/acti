											value="@{conhecimentoController.selected.serie}" />
									</hbox>
								</hbox>
								<hbox>
									<label value="Sub Serie: " />
									<textbox constraint=""
										style="text-transform:uppercase"
										value="@{conhecimentoController.selected.subSerie}" />
								</hbox>
							</row>
							<row>
								<hbox>
									<label value="Tipo de Frete: " />
									<radiogroup
										selectedItem="@{conhecimentoController.selected.tipoFrete}">
										<radio label="FOB" value="FOB" />
										<radio label="CIF" value="CIF" />
									</radiogroup>
								</hbox>
								<hbox>
									<label value="Tipo Gris: " />
									<combobox
										selectedItem="@{conhecimentoController.selected.tipoGris}">
										<comboitem label="Liberado"
											description="" value="LIBERADO" />
										<comboitem label="Rastreado"
											description="" value="RASTREADO" />
										<comboitem label="Telemonitorado"
											description="" value="TELEMONITORADO" />
										<comboitem
											label="Rastreado com escolta" description=""
											value="RASTREADO COM ESCOLTA" />
									</combobox>
								</hbox>
								<hbox>
									<label value="C.F.O.P: " />
									<combobox
										selectedItem="@{conhecimentoController.selected.cpof}"
										autodrop="true">
	
										<comboitem
											forEach="${conhecimentoController.cpofs}"
											label="${each.codigo}" description="${each.descricao}"
											value="${each}" />
									</combobox>
								</hbox>
							</row>
							<row>
								<label value="Local de Emissão " />
								<textbox
									style="text-transform:uppercase"
									value="@{conhecimentoController.selected.localEmissao}" />
								<label value="Data/Hora: " />
								<datebox cols="25"
									format="dd/MM/yyyy a HH:mm"
									onCreate="self.setValue(new Date())" mold="rounded"
									disabled="true"
									value="@{conhecimentoController.selected.dataCriacao}" />
							</row>
							<row>
								<label value="Digitador: " />
								<combobox
									selectedItem="@{conhecimentoController.selected.digitador}">
									<comboitem
										forEach="${conhecimentoController.digitadores}"
										label="${each.nome}" description="${each.login}"
										value="${each}" />
